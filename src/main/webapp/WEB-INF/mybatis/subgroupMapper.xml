<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SubGroupFacade">
	<select id="getSubGroupByUpperGroupId"  resultType="web.app.pkvalve.domains.SubGroup" parameterType="String">
		select
			subGroupCode,
			subGroupName
		from
			subgroup
		where
			upperGroupCode = #{upperGroupCode}
	</select>
</mapper>