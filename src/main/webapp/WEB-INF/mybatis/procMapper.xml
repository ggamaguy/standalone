<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ProcFacade">
	<select id="getAllProcList" resultType="web.app.pkvalve.domains.Proc">
		select 
			processCode,
			processName
		from 
			proc
	</select>
</mapper>